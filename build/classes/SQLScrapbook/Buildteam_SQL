show databases;
use buildteam;

create table email_list(
	email_id INT NOT NULL AUTO_INCREMENT,
   email_address VARCHAR(40) NOT NULL,
   first_name VARCHAR(100) NOT NULL,
   last_name VARCHAR(100) NOT NULL,
   PRIMARY KEY ( email_id )
);

